;выключение компьютера
__shutdown equ 0050h:0380h

;перезагрузка компьютера
__reboot equ 0050h:0383h

;выводит на экран символ(!1) цветом(!2) по х(!3) и у(!4) 
__print_char equ 0050h:0386h

;очищает экран
__clear_screen equ 0050h:0389h

;считывает символ клавиатуры, возвращает скан-код(ah) и ascii(al)
__read_key equ 0050h:038ch

;считывает сектор по lba(!1) в адрес м(!2) и б(!3)
__read_sect equ 0050h:038fh

;записывает сектор по lba(!1) из адреса м(!2) и б(!3)
__write_sect equ 0050h:0392h

;запускает программу по ее адресу(!1)
__run_program equ 0050h:0395h

; возврат в desktop
__exit_program equ 0050h:0398h